<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
		android:layout_height="wrap_content">
	<TextView
			android:text="Exercise Name"
			android:maxWidth="100dp"
			android:layout_width="100dp"
			android:layout_height="wrap_content"
			android:id="@+id/exercise_name_textview_workout"
			android:textAlignment="center"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="11dp"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
	<TextView
			android:text="X"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAlignment="center"
			android:id="@+id/x_textview_workout"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
			app:layout_constraintStart_toEndOf="@+id/exercise_name_textview_workout"
			app:layout_constraintEnd_toStartOf="@+id/rpe_textview_workout" android:layout_marginEnd="15dp"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
	<TextView
			android:text="20"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/rpe_textview_workout"
			android:textAlignment="center"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
			app:layout_constraintEnd_toStartOf="@+id/weight_input_workout" android:layout_marginEnd="52dp"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
	<TextView
			android:text="5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/sets_textview_workout"
			app:layout_constraintEnd_toStartOf="@+id/x_textview_workout" android:textAlignment="center"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
	<TextView
			android:text="10"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/reps_textview_workout"
			app:layout_constraintStart_toEndOf="@+id/x_textview_workout" android:textAlignment="center"
			app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
			android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
	<TextView
			android:text="lbs"
			android:layout_width="36dp"
			android:layout_height="wrap_content"
			android:id="@+id/weight_input_workout"
			android:textAlignment="center" android:textSize="14sp"
			android:textStyle="bold"
			android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="19dp" android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/textView3"/>
</android.support.constraint.ConstraintLayout>